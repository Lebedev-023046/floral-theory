{
	"compilerOptions": {
		"lib": ["DOM", "ES2020"],
		"jsx": "react-jsx",
		"module": "ESNext",
		"target": "ES6",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"allowJs": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"skipLibCheck": true
	}
}
